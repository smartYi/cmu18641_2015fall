package client;


import java.net.Socket;

public class CarModelOptionsIO {
	
	DefaultSocketClient defaultSocketClient = null;
	
	/**
	 * Constructor with socket as argument.
	 * @param socket
	 */
	public CarModelOptionsIO(Socket socket) {
		defaultSocketClient = new DefaultSocketClient(socket);
	}
	
	/**
	 * This method is to get different model names.
	 * @param fileSets
	 * @return
	 */
	public void getFileName(String fileSets) {
		defaultSocketClient.getFileName(fileSets);
	}
	
	/**
	 * This method is to read property from property file and transfer the object from client to server.
	 */
	public void readProperty() {
		defaultSocketClient.readProperty();
	}
}
