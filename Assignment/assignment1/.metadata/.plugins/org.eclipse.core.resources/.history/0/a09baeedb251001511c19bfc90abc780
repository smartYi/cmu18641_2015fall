package parkingSimulator;

public class parkingTicket {
	parkedCar legalcar;
	public parkingTicket(parkedCar parkedcar){
		this.legalcar = parkedcar;
	}
	
	public String toString(){
		return ("Car maker is "+legalcar.getMake()+", car model is "+legalcar.getModel()+", car color is "+legalcar.getColor()+", car license number is"+legalcar.getLicenseNumber());
	}
}
