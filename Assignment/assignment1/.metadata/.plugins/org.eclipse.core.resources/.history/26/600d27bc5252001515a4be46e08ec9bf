package parkingSimulator;

public class test {
	public static void main(String[] args){
		private parkingMeter parkingmeter = new parkingMeter();
		private parkedCar mycar = new parkedCar();
		policeOfficer police = new policeOfficer("John","5901");
		parkingTicket parkingticket = new parkingTicket();
		parkingmeter.setPurchasedMinutes(100);
		mycar.setMake("Toyota");
		mycar.setModel("GTR");
		mycar.setColor("red");
		mycar.setLicenseNumber("PHD-ING");
		mycar.setParkedMinutes(281);
		police.issueTicket(mycar, parkingmeter,parkingticket);
		
	}
}
